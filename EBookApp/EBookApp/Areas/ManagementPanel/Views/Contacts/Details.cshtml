@model EBook.Entities.Models.Contact

@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/ManagementPanel/Views/Shared/_Layout.cshtml";
}

<h1>Detay</h1>

<div>
    <h4>Contact</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
           Başlık
        </dt>
        <dd class = "col-sm-10">
            @Model.Title
        </dd>
        <dt class = "col-sm-2">
           Alt Başlık
        </dt>
        <dd class = "col-sm-10">
            @Model.Subtitle
        </dd>
        <dt class = "col-sm-2">
            Adres
        </dt>
        <dd class = "col-sm-10">
            @Model.Address
        </dd>
        <dt class = "col-sm-2">
            Email
        </dt>
        <dd class = "col-sm-10">
            @Model.Email
        </dd>
        <dt class = "col-sm-2">
            Telefon
        </dt>
        <dd class = "col-sm-10">
            @Model.Phone
        </dd>
        <dt class = "col-sm-2">
            Durum
        </dt>
        <dd class = "col-sm-10">
            @Model.Status
        </dd>
        <dt class = "col-sm-2">
            Ekleme Tarihi
        </dt>
        <dd class = "col-sm-10">
            @Model.CreatedDate
        </dd>
    </dl>
</div>

